{
    "name": "delboy1978uk/bone-controller",
    "description": "Controller package for Bone Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "php": ">7.2",
        "delboy1978uk/barnacle": "^2.3",
        "delboy1978uk/bone-db": "^1.2",
        "delboy1978uk/bone-i18n": "^1.1",
        "delboy1978uk/bone-log": "^1.0",
        "delboy1978uk/bone-server": "^1.0",
        "delboy1978uk/bone-view": "^1.0",
        "delboy1978uk/session": "^1.1",
        "laminas/laminas-diactoros": "2.*",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Bone\\Controller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoneTest\\": "tests/unit/"
        }
    }
