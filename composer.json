{
    "name": "delboy1978uk/bone-controller",
    "description": "Controller package for Bone Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "boneframework/contracts": "^1.4",
        "delboy1978uk/barnacle": "^2.3",
        "delboy1978uk/bone-db": "^1.2",
        "delboy1978uk/bone-i18n": "^1.2",
        "delboy1978uk/bone-log": "^1.3",
        "delboy1978uk/bone-server": "^1.2",
        "delboy1978uk/bone-view": "^1.7",
        "delboy1978uk/session": "^1.6",
        "jms/serializer": "^3.32",
        "laminas/laminas-diactoros": "^3.3",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "delboy1978uk/dev-tools": "^v1.0",
        "roave/security-advisories": "dev-latest",
        "delboy1978uk/bone-doctrine": "v2.3"
    },
    "autoload": {
        "psr-4": {
            "Bone\\Controller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bone\\Test\\Controller\\": "tests/unit/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": false
        }
    }
}
